if [ -z "$1" ] ; then echo erhe
else
if [ -z "$2" ] ;
  then puoli="front"
  else puoli="$2"
fi
rm -f gcode.gcode
rm -f front.ngc
rm -f back.ngc
pcb2gcode --$puoli $1 --metric --zsafe=3.0 \
--zwork=-0.1 --zcut=-1.7 --offset=0.1 \
--zchange 0.1 --mill-feed 1 --mill-speed 1
python3 corret_place.py $puoli.ngc gcode1.gcode
python3 laseroi.py gcode1.gcode > gcode.gcode
rm -f gcode1.gcode
python3 showgcode_inch.py
fi