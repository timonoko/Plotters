if [ -z "$1" ] ; then echo erhe
else
pcb2gcode --front $1 --metric --zsafe=3.0 \
--zwork=-0.1   \
 --zcut=-1.7 --offset=0.1 \
--zchange 0.1 --mill-feed 1 --mill-speed 1
python3 corret_place.py front.ngc gcode1.gcode
